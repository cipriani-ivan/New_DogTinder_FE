{
  "projectType": "library",
  "root": "libs/owners",
  "sourceRoot": "libs/owners/src",
  "prefix": "ad",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/owners"],
      "options": {
        "project": "libs/owners/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/owners/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/owners/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/owners"],
      "options": {
        "jestConfig": "libs/owners/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/owners/**/*.ts", "libs/owners/**/*.html"]
      }
    }
  },
  "tags": ["ad"]
}
